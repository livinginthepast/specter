#!/usr/bin/env bash
set -e
set -u
set -o pipefail

trap "exit" INT

source ".medic/_support/doctor.sh"

bin/dev/audit
bin/dev/update
bin/dev/test

step "Pushing" "git push origin HEAD"

cecho --bright-green "\nDone!" --bright-yellow "Check CI at" --bright-bold-white "https://github.com/livinginthepast/specter/actions"
